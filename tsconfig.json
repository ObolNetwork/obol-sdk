{
  "compilerOptions": {
    "target": "es2016",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution":"node",
    "baseUrl": "./",
    "paths": {
      "@safe-global/types-kit/types": ["node_modules/@safe-global/types-kit/dist/src/types.d.ts"]
    }
  },
  "exclude": ["node_modules", "dist", "test/sdk-package", "test/nextjs-test-app"]
}
